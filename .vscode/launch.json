{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Gateway",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/gateway/bin/Debug/net9.0/gateway.dll",
      "cwd": "${workspaceFolder}/gateway",
      "stopAtEntry": false,
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    {
      "name": "Launch Big Data",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/big_data/bin/Debug/net9.0/big_data.dll",
      "cwd": "${workspaceFolder}/big_data",
      "stopAtEntry": false,
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    }
  ]
}
